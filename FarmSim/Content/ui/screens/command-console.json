    {
      "$type": "UI.ColoredPanel, UI",
      "color": "black",
      "alpha": 128,
      "width": "100%",
      "height": "100%",
      "Children": [
        {
          "$type": "UI.ScrollableContainer, UI",
          "width": "100%",
          "height": "100% - 42",
          "bottom": 42,
          "margin": 10,
          "scrollToBottom": true,
          "scrollboxTexture": "scrollbox",
          "scrollbarBackgroundTexture": "scrollbar-background",
          "Children": [
            {
              "$type": "UI.Log, UI",
              "id": "command-log"
            }
          ]
        },
        {
          "$type": "UI.ColoredPanel, UI",
          "color": "white",
          "width": "100%",
          "height": 1,
          "bottom": 41
        },
        {
          "$type": "UI.TextInput, UI",
          "id": "command-input",
          "color": "white",
          "placeholder": "Enter command. Type \"/?\" for help...",
          "width": "100%",
          "bottom": 0,
          "margin": 10
        }
      ]
    }
  ]
}